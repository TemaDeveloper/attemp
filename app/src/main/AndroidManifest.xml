<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com_country_city.country_city_java.countrycitygame">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CountryCityGame.Fullscreen">
        <activity
            android:name=".Main.FAQActivity"
            android:exported="false" />
        <activity
            android:name=".Main.FinishActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".OnBoarding.OnBoardingActivity"
            android:exported="true"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Main.LvlActivity"
            android:exported="false"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustResize" />
        <activity
            android:name=".DifficultyLvls.LightLvlsActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Main.MainActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
    </application>

</manifest>